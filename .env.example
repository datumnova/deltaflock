# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# Never commit .env files to version control!

# Database configuration
database_url=duckdb:///:memory:
duckdb_path=./data/golduck.db

# Database initialization configuration
# Provide path to a custom initialization SQL file
# duckdb_init_sql_file=/path/to/your/custom-init.sql

# If not provided, the bundled duckdb_init.sql file will be used

# Unity Catalog configuration
uc_catalog_endpoint=https://your-unity-catalog-endpoint.databricks.com
uc_catalog_token=dapi1234567890abcdef1234567890abcdef12345678  # Example format

# Azure configuration
azure_storage_account_name=your_storage_account_name
azure_storage_account_key=abcd1234567890efgh1234567890ijkl==  # Example format
azure_account_name=your_azure_account
azure_client_id=12345678-1234-1234-1234-123456789012  # Example GUID format
azure_client_secret=your_azure_client_secret_here
azure_tenant_id=87654321-4321-4321-4321-210987654321  # Example GUID format

# Telemetry configuration
telemetry_enabled=true
telemetry_endpoint=http://localhost:4318
telemetry_service_name=golduck
telemetry_service_version=1.0.0


# Cache configuration
enable_query_cache=true
cache_directory=./query_cache
cache_expiry_hours=24
max_cache_size_mb=1000
cache_cleanup_interval_hours=6
cache_backend=file
redis_url=redis://localhost:6379/0

# Admin configuration
admin_api_key=your_secure_admin_api_key_here
